{
    "version": 2,
    "builds": [
      {
        
        "src": "api/generate.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        
        "src":  "/generate",
        "methods": ["POST"],
        "dest": "/api/generate.js"
      },
      {
        
        "src":  "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  